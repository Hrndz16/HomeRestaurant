/* Estilo para QListWidget */
QListWidget {
    font-size: 14px;
    color: rgb(50, 50, 50);
    background-color: rgb(240, 240, 240);
    border: 2px solid rgb(173, 216, 230);
    border-radius: 5px;
    padding: 8px;
    min-width: 150px; /* Ancho mínimo para asegurar que se vean los elementos */
}

/* Efecto al pasar el mouse sobre un elemento en QListWidget */
QListWidget::item:hover {
    background-color: rgb(200, 220, 240);
}

/* Efecto al seleccionar un elemento en QListWidget */
QListWidget::item:selected {
    background-color: rgb(150, 200, 220);
    color: white;
}

/* Estilo para QLabel */
QLabel {
    font-size: 16px;
    font-weight: bold;
    color: rgb(50, 50, 50);
    background-color: rgb(240, 240, 240);
    padding: 10px;
}

/* Estilo para QPushButton */
QPushButton {
    font-size: 14px;
    font-weight: bold;
    color: white;
    background-color: rgb(173, 216, 230);
    border: none;
    border-radius: 8px;
    padding: 10px 20px;
}

/* Efecto al pasar el mouse sobre el botón */
QPushButton:hover {
    background-color: rgb(150, 200, 220);
}

/* Efecto al presionar el botón */
QPushButton:pressed {
    background-color: rgb(130, 180, 200);
}
/* Estilo para QLineEdit */
QLineEdit {
    font-size: 14px;
    color: rgb(50, 50, 50);
    background-color: rgb(240, 240, 240);
    border: 2px solid rgb(173, 216, 230);
    border-radius: 5px;
    padding: 8px;
}

/* Efecto al pasar el mouse sobre el QLineEdit */
QLineEdit:hover {
    border: 2px solid rgb(150, 200, 220);
}

/* Efecto al estar enfocado en el QLineEdit */
QLineEdit:focus {
    border: 2px solid rgb(50, 150, 220);
}
/* Estilo para QComboBox */
QComboBox {
    font-size: 14px;
    color: rgb(50, 50, 50);
    background-color: rgb(240, 240, 240);
    border: 2px solid rgb(173, 216, 230);
    border-radius: 5px;
    padding: 8px;
}

/* Estilo para el desplegable del QComboBox */
QComboBox::drop-down {
    background-color: rgb(220, 220, 220);
    border: none;
    border-radius: 5px;
}

/* Efecto al pasar el mouse sobre el QComboBox */
QComboBox:hover {
    border: 2px solid rgb(150, 200, 220);
}

